const express = require("express")
const router = express.Router()

const db = require("../db")

router.delete("/{{ . }}/:id", (req, res) => {
    const id = req.params.id
    await db.query(
        "DELETE FROM {{ . }} WHERE id = $1",
        [ id ]
    )

    res.json({ message: "success" })
})

module.exports = router
